generator client {
  provider = "prisma-client-js"
    binaryTargets = ["native", "debian-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model LoginHistory {
   id           String   @id @default(cuid())
  userId    String
  email     String
  role      String
  ip        String?
  userAgent String?
  loginAt   DateTime
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}

